{
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/favicon.ico",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "dest": "/favicon.ico"
        },
        {
            "src": "/",
            "dest": "main.py"
        },
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
